        <%}else if(/\.ico$/.test(iconurl)){%>
        <link rel="shortcut icon" href="<%=relative_path%><%=pkg.idoc.logo%>" type="image/x-icon">
        <%}%>
    <%}%>
    <link rel="stylesheet" type="text/css" href="<%=relative_path%>static/css/main.css">
</head>
